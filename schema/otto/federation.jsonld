{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "https://[OTTO]/federation",
      "@type": "rdfs:Class",
      "rdfs:comment": "A legal entity that can manage the organizations.",
      "rdfs:label": "federation",
      "rdfs:subClassOf": {
        "@id": "http://schema.org/Organization"
      }
    },
    {
      "@id": "https://schema.org/name",
      "@type": "rdf:Property",
      "http://schema.org/domainIncludes": [
        {
          "@id": "https://[OTTO]/federation"
        }
      ],
      "rdfs:comment": "Name of the federation",
      "rdfs:label": "name"
    },
    {
      "@id": "http://[OTTO]/participant",
      "@type": "rdf:Property",
      "http://schema.org/domainIncludes": [
        {
          "@id": "https://[OTTO]/federation"
        }
      ],
      "rdfs:comment": "It represents the list of organization that are added under the federation as a participants.",
      "rdfs:label": "participants"
    },
    {
      "@id": "http://[OTTO]/entities",
      "@type": "rdf:Property",
      "http://schema.org/domainIncludes": [
        {
          "@id": "https://[OTTO]/federation"
        }
      ],
      "http://schema.org/rangeIncludes": {
        "@id": "http://[OTTO]/Entity"
      },
      "rdfs:comment": "It represents the list of open id providers, RPs, etc that are added under the federations.",
      "rdfs:label": "entities"
    },
    {
      "@id": "http://schema.org/organization",
      "@type": "rdf:Property",
      "http://schema.org/domainIncludes": [
        {
          "@id": "https://[OTTO]/federation"
        }
      ],
      "http://schema.org/rangeIncludes": {
        "@id": "http://[OTTO]/participant"
      },
      "rdfs:comment": "It represent the owner organization for the federation.",
      "rdfs:label": "organization"
    }
  ]
}